<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    
    	<!--Marketing Target Obect Master  -->
    	
        <record id="view_marketing_target_object_form" model="ir.ui.view">
            <field name="name">view.marketing.target.object.form</field>
            <field name="model">marketing.target.object</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
            	<form string="Marketing Target">
   					<group>
   						<field name="name" required="1"/>
   						<field name="model_id" options="{'no_create':True,'no_open':True}"/>
   						<field name="sequence" />
   						<field name="description"/>
   					</group>
            	</form>
	        </field>
		</record>
    
		<record id="view_marketing_target_object_tree" model="ir.ui.view">
			<field name="name">view.marketing.target.object.tree</field>
			<field name="model">marketing.target.object</field>
			<field name="arch" type="xml">
				<tree string="Marketing Target Object">
					<field name="sequence" widget="handle"/>
					<field name="name"/>
					<field name="model_id" options="{'no_create':True,'no_open':True}"/>
				</tree>
			</field>
		</record>    
    
		<record id="open_action_marketing_target_object" model="ir.actions.act_window">
			<field name="name">Target Object</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">marketing.target.object</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="help" type="html">
				<p class="oe_view_nocontent_create">
					Click to add a new Target Object.
				</p>
			</field>
		</record>
    
    	<!-- Marketing Target -->
    	
        <record id="view_marketing_target_form" model="ir.ui.view">
            <field name="name">view.marketing.target.form</field>
            <field name="model">marketing.target</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
            	<form>
	            	<sheet>
	            		<group>
	            			<group>
	            				<field name="name" required="1"/>
	            				<field name="sequence"/>
	            				<field name="target_type" required="1"/>
	            			</group>
	            			<group>
	            				<field name="academic_year_id" options="{'no_create':True,'no_open':True}" readonly="1"/>
	            			</group>
	            		</group>            		
	            		<notebook>
							<page string="Target Details">
								<field name="target_line_ids" required="1" context="{'marketing_target':True}">
									<tree editable="bottom">
										<field name="target_id" invisible="1"/>
										<field name="admission_model_id" invisible="1"/>
										<field name="target_ref" options="{'no_create':True, 'no_open':True}" required="1"/>
										<field name="admission_field" domain="[('model_id','=',admission_model_id)]" required="1" 
											options="{'no_create':True,'no_open':True}" context="{'marketing_target':True}"/>
										<field name="logical_condition"/>
									</tree>
								</field>
							</page>
						</notebook>
	            	</sheet>
            	</form>
	        </field>
		</record>
    
		<record id="view_marketing_target_tree" model="ir.ui.view">
			<field name="name">view.marketing.target.tree</field>
			<field name="model">marketing.target</field>
			<field name="arch" type="xml">
				<tree string="Marketing Target">
					<field name="sequence" widget="handle"/>
					<field name="name"/>
					<field name="academic_year_id"/>
					<field name="target_type"/>
					<!-- <field name="state"/> -->
				</tree>
			</field>
		</record>    
    
