<odoo>
	<data noupdate="1">
	
		<!-- Expenditure Proposal sequence -->
        <record id="sequence_expenditure_proposal" model="ir.sequence">
            <field name="name">Expenditure Proposal Sequence</field>
            <field name="code">expenditure.proposal</field>
            <!-- <field name="prefix">EP</field> -->
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>	
        
		<!-- Travelling Expenditure Proposal sequence -->
        <record id="sequence_travelling_expenditure_proposal" model="ir.sequence">
            <field name="name">Travelling Expenditure Proposal Sequence</field>
            <field name="code">travelling.expenditure.proposal</field>
            <!-- <field name="prefix">ATP</field> -->
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>	        
        
	</data>
</odoo>