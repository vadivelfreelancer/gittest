<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
	
		<record id="view_attendance_histrory_table_tree" model="ir.ui.view">
			<field name="name">view.attendance.history.table.tree</field>
			<field name="model">attendance.history.table</field>
			<field name="arch" type="xml">
				<tree string="Attendance History Table">
					<field name="fixedstr1"/>
					<field name="event_dt"/>
					<field name="event_time"/>
					<field name="controller_name"/>
					<field name="fixedstr2"/>
					<field name="userid"/>
					<field name="username"/>
					<field name="department"/>
					<field name="access_mode"/>
					<field name="doorstate"/>
					<field name="functionkey"/>
					<field name="description"/>
					<field name="userid1"/>
					<field name="flag"/>
				</tree>
			</field>
		</record>
	
	
		<record id="view_attendance_histrory_table_form" model="ir.ui.view">
			<field name="name">view.attendance.history.table.form</field>
			<field name="model">attendance.history.table</field>
			<field name="arch" type="xml">
				<form string="Attendance History Table">
					<group>
						<group>
							<field name="fixedstr1"/>
							<field name="event_dt"/>
							<field name="event_time"/>
							<field name="controller_name"/>
							<field name="fixedstr2"/>
							<field name="userid"/>
							<field name="username"/>
						</group>
						<group>
							<field name="department"/>
							<field name="access_mode"/>
							<field name="doorstate"/>
							<field name="functionkey"/>
							<field name="description"/>
							<field name="userid1"/>
							<field name="flag"/>
						</group>
					</group>
				</form>
			</field>
		</record>
	
		<record id="action_attendance_histrory_table" model="ir.actions.act_window">
			<field name="name">Attendance History Table</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">attendance.history.table</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="help" type="html">
				<p class="oe_view_nocontent_create">
					Click to create a new attendance history.
				</p>
			</field>
		</record>
	
	</data>
</odoo>