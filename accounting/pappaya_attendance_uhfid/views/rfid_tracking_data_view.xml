<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_pappaya_rfid_data_tree" model="ir.ui.view">
            <field name="name">pappaya.rfid.data.tree</field>
            <field name="model">pappaya.rfid.tracking.data</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <tree string="RFID Card Data" create="false" edit="false">
                    <field name="student_id" />
                    <field name="rfid_card_id" />
                    <field name="capture_datetime" />

                </tree>
            </field>
        </record>
       
        <record id="view_pappaya_rfid_data_search" model="ir.ui.view">
            <field name="name">pappaya.rfid.data.search</field>
            <field name="model">pappaya.rfid.tracking.data</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <search string="RFID Data">
                   
                    <field name="student_id" />
                    <field name="rfid_card_id" />
                    
                    <newline />
                    <group expand="0" string="Group By...">
                        <filter string="Student" domain="[]" context="{'group_by':'student_id'}" />
                        <filter string="Card No." domain="[]"
                            context="{'group_by':'rfid_card_id'}" />
                        <!--<filter string="Course" domain="[]" context="{'group_by':'grade_id'}" />-->
                        <!--<filter string="Batch" domain="[]"-->
                            <!--context="{'group_by':'section_id'}" />-->
                        
                    </group>
                </search>
            </field>
        </record>
        <record model="ir.actions.act_window" id="act_open_pappaya_rfid_data_view">
            <field name="name">RFID Access Card Data</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">pappaya.rfid.tracking.data</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree</field>
            <field name="search_view_id" ref="view_pappaya_rfid_data_search" />
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>
<!--
        <record model="ir.actions.act_window.view" id="act_open_pappaya_attendance_line_view_form">
            <field name="act_window_id" ref="act_open_pappaya_attendance_line_view" />
            <field name="sequence" eval="20" />
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_pappaya_attendance_line_form" />
        </record>
-->
        <record model="ir.actions.act_window.view" id="act_open_pappaya_rfid_data_view_tree">
            <field name="act_window_id" ref="act_open_pappaya_rfid_data_view" />
            <field name="sequence" eval="10" />
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_pappaya_rfid_data_tree" />
        </record>
    </data>
</odoo>
