<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

        <!-- Form Views -->
        <record id="view_nconnect_story_form" model="ir.ui.view">
            <field name="name">view.nconnect.story.form</field>
            <field name="model">pappaya.story</field>
            <field name="arch" type="xml">
                <form string="Story">
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="date"/>
                            <field name="image" widget="image" class="oe_avatar"
                            options="{'preview_image': 'image_small', 'size': [3000, 3000]}"/>
                        </group>
                        <group>
                            <field name="description"/>
                            <field name="grade_id"/>
                            <field name="section_id"/>
                        </group>
                        <group>
                            <field name="images" widget="many2many_binary"/>
                        </group>
                    </group>
                </form>
            </field>
        </record>
        
        <record id="view_nconnect_student_form" model="ir.ui.view">
            <field name="name">view.nconnect.student.form</field>
            <field name="model">pappaya.student</field>
            <field name="arch" type="xml">
                <form string="Student">

                    <group string="Basic Details">
                        <group>
                            <field name="student_id" readonly="1"/>
                            <field name="gender" widget="radio"/>
                            <field name="blood_group"/>
                        </group>
                        <group>
                            <field name="name"/>
                            <field name="dob"/>
                            <field name="image"/>
                            <field name="school_id"/>
                        </group>
                    </group>

                    <group string="Parent Details">
                        <group>
                            <field name="father"/>
                            <field name="father_mobile"/>
                            <field name="father_mail"/>
                            <field name="father_id"/>
                            <field name="address1"/>
                        </group>
                        <group>
                            <field name="mother"/>
                            <field name="mother_mobile"/>
                            <field name="mother_mail"/>
                            <field name="mother_id"/>
                            <field name="address2"/>
                        </group>
                    </group>

                    <group string="Education Details">
                        <group>
                            <field name="grade_id"/>
                            <field name="document"/>
                        </group>
                        <group>
                            <field name="section_id"/>
                            <field name="class_teacher_id"/>
                        </group>
                    </group>

                </form>
            </field>
        </record>

        <record id="view_nconnect_teacher_form" model="ir.ui.view">
            <field name="name">view.nconnect.teacher.form</field>
            <field name="model">pappaya.teacher</field>
            <field name="arch" type="xml">
                <form string="Teacher">

                    <group string="Basic Details">
                        <group>
                            <field name="teacher_id" readonly="1"/>
                            <field name="gender" widget="radio"/>
                            <field name="blood_group"/>
                            <field name="is_principal"/>
                        </group>
                        <group>
                            <field name="name"/>
                            <field name="dob"/>
                            <field name="image"/>
                            <field name="school_id"/>
                        </group>
                    </group>

                    <group string="Contact Details">
                        <group>
                            <field name="mobile"/>
                            <field name="address1"/>
                            <field name="mobile_emergency"/>
                        </group>
                        <group>
                            <field name="email"/>
                            <field name="address2"/>
                        </group>
                    </group>

                    <group string="Education Details">
                        <group>
                            <field name="qualification"/>
                            <field name="document"/>
                        </group>
                        <group>
                            <field name="specialization"/>
                        </group>
                    </group>

                    <group string="Assign Class">
                        <group>
                            <field name="subject_assign_ids"/>
                        </group>
                        <group>
                            <field name="class_assign_ids">
                                <tree>
                                    <field name="grade_id"/>
                                    <field name="section_id"/>
                                </tree>
                            </field>
                        </group>
                    </group>

                    <group string="Class Teacher to">
                        <group>
                            <field name="grade_id"/>
                        </group>
                        <group>
                            <field name="section_id"/>
                        </group>
                    </group>

                </form>
            </field>
        </record>

        <record id="view_nconnect_teacher_class_form" model="ir.ui.view">
            <field name="name">view.nconnect.teacher.class.form</field>
            <field name="model">pappaya.teacher.class</field>
            <field name="arch" type="xml">
