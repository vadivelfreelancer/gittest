        <!-- Sequences for iMulPay Ref. No. -->
        <record id="seq_imulpay_authorize" model="ir.sequence">
            <field name="name">iMulPay</field>
            <field name="code">imulpay.authorize</field>
            <field name="padding">7</field>
             <field name="implementation">no_gap</field>
            <field name="active">True</field>
            <field name="number_next_actual">1000001</field>
        </record>

        <!-- Sequences for POS payment Ref. No. -->
        <record id="seq_ezepay_pos_reference" model="ir.sequence">
            <field name="name">POS Payment Reference</field>
            <field name="code">pappaya.ezepay.payment.ref</field>
            <field name="padding">16</field>
             <field name="implementation">no_gap</field>
            <field name="active">True</field>
        </record>
        
        
