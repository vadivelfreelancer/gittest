<?xml version="1.0"?>
<odoo>
    <data>

		<record model="ir.ui.view" id="student_fee_collection_summary_report_view">
			<field name="name">student.fee.collection.summary.form</field>
			<field name="model">student.fee.collection.summary</field>
			<field name="arch" type="xml">
				<form string="Student Fee Collection Summary Report">
					 <group>
						 <group>
							 <!--<field name="society_ids" widget="many2many_tags" domain="[('type','=','society')]" options="{'no_open': True,'no_create':True}" />-->
							 <field name="school_ids" widget="many2many_tags" domain="[('type','=','branch')]" options="{'no_open': True,'no_create':True}" />
							 <field name="from_date" class="asterik"/>
							 <field name="to_date" class="asterik"/>
						 </group>
						 <group/>
						 </group>
					 <footer>
						 <button invisible="1" string="PDF" name="generate_pdf_report" class="oe_highlight" type="object"/>
						 <button name="fees_collection_detail_excel_report" type="object" string="Excel" class="oe_highlight" />
						 <button string="Cancel" special="cancel" />
					</footer>
			 	</form>
			</field>
		</record>

		<record model="ir.actions.act_window" id="student_fee_collection_summary_report_act">
            <field name="name">Fee collection Details</field>
            <field name="res_model">student.fee.collection.summary</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="student_fee_collection_summary_report_view" />
            <field name="target">new</field>
        </record>
	
    </data>
</odoo>
