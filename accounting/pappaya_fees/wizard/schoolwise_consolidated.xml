<odoo>
    <data>

        <record id="view_schoolwise_consolidated_wizard_report" model="ir.ui.view">
            <field name="name">Schoolwise Consolidated Report</field>
            <field name="model">schoolwise.consolidated.wizard</field>
            <field name="arch" type="xml">
                <form string="Schoolwise Consolidated Report">
                    <group>
                        <group>
                            <!--<field name="society_ids" widget="many2many_tags" domain="[('type','=','society')]" options="{'no_open': True,'no_create':True}"/>-->
                            <field name="school_ids" widget="many2many_tags" domain="[('type','=','branch')]" options="{'no_open': True,'no_create':True}"/>
                            <field name="academic_year_ids" widget="many2many_tags"  options="{'no_open': True,'no_create':True}"/>
                            <!-- <field name="grade_ids" widget="many2many_tags"  options="{'no_open': True,'no_create':True}"/> -->
                        </group>
                        <group></group>
                    </group>
                    <footer>
                        <button string="Excel" name="generate_excel_report" class="oe_highlight" type="object"/>
                        <button string="Cancel" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_schoolwise_consolidated_wizard_report" model="ir.actions.act_window">
            <field name="name">Schoolwise Consolidated Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">schoolwise.consolidated.wizard</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

    </data>
</odoo>
