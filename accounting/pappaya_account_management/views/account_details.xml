        <record id="account.view_account_group_form" model="ir.ui.view">
            <field name="name">account.group.form</field>
            <field name="model">account.group</field>
            <field name="arch" type="xml">
            <form string="Ledger">
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="sub_group_id" required="1" options="{'no_open': True,'no_create':True}" />
                        <field name="color" />
                        <field name="description" />
                        <field name="code_prefix" invisible="1" />
                        <field name="parent_id" invisible="1" />
                    </group>
                    <group>
                        <group>
                            <field name="is_cash" required="1" widget="radio" options="{'horizontal': true}" class="col-md-8 oe_inline"/>
                            <field name="is_bank" required="1" widget="radio" options="{'horizontal': true}" class="col-md-8 oe_inline"/>
                            <field name="is_loan" required="1" widget="radio" options="{'horizontal': true}" class="col-md-8 oe_inline"/>
                            <field name="is_fdr" required="1" widget="radio" options="{'horizontal': true}" class="col-md-8 oe_inline"/>
                        </group>
                        <group>
                            <field name="is_unsecured_loan" required="1" widget="radio" options="{'horizontal': true}" class="col-md-8 oe_inline"/>
                            <field name="is_sundry_debtor" required="1" widget="radio" options="{'horizontal': true}" class="col-md-8 oe_inline"/>
                            <field name="is_sub_ledger" required="1" widget="radio" options="{'horizontal': true}" class="col-md-8 oe_inline"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>


