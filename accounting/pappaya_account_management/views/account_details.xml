    <!--account branch subledger mapping master-->
    <record id="accounting_branch_ledger_mapping_master_form" model="ir.ui.view">
        <field name="name">Branch Sub Ledger Mapping </field>
        <field name="model">branch.ledger</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group string="Branch Sub Ledger Mapping">
                        <group>
                            <field name="financial_year" options='{"no_open": True, "no_create": True}'/>
                        </group>
                         <group  invisible="context.get('subledger', True)">
                            <field name="branch" options='{"no_open": True, "no_create": True}'
                             invisible="context.get('subledger', True)"/>
                        </group>
                         <group invisible="context.get('branch', True)">
                           <field name="sub_ledger" options='{"no_open": True, "no_create": True}'
                                  invisible="context.get('branch', True)"/>
                        </group>
                    </group>
                    <notebook>
                        <page string="Branch" invisible="context.get('branch', True)">
                            <field name="branch_ledger">
                                <tree editable="bottom">
                                    <field name="branch" options='{"no_open": True, "no_create": True}'
                                   invisible="context.get('branch', True)"/>
                                </tree>
                            </field>
                        </page>
                        <page string="Sub Ledger" invisible="context.get('subledger', True)">
                            <field name="ledger_branches">
                                <tree editable="bottom">
                                     <field name="ledger" options='{"no_open": True, "no_create": True}'
                                      invisible="context.get('subledger', True)"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>
