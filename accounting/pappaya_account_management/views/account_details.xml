    <!--account subledger financialyear mapping master-->
    <record id="accounting_ledger_financial_mapping_master_form" model="ir.ui.view">
        <field name="name"> Sub Ledger Financial Year Mapping </field>
        <field name="model">financial.year</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name" readonly="1" options='{"no_open": True, "no_create": True}'/>
                        </group>
                    </group>
                    <notebook>
                        <page string="Sub Ledger" invisible="context.get('branch_financial', True)">
                            <field name="ledger_financial">
                                <tree editable="bottom">
                                    <field name="name" options='{"no_open": True, "no_create": True}'/>
                                </tree>
                            </field>
                        </page>
                        <page string="Branch Details" invisible="context.get('subledger', True)">
                            <field name="branch_financial">
                            </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>
     <record id="accounting_ledger_financial_mapping_master_tree" model="ir.ui.view">
