    <!--Cheque Lot master-->
    <record id="subledger_cheque_book_form" model="ir.ui.view">
        <field name="name">SubLedger Cheque Book </field>
        <field name="model">subledger.cheque.book</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form>
                <header>
                    <button name="action_generate" string="Generate" type="object" attrs="{'invisible': [('is_created', '=', True)]}"/>
                </header>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="ledger_id" options='{"no_open": True, "no_create": True}'/>
                            <field name="sub_ledger_id" options='{"no_open": True, "no_create": True}'/>
                            <field name="dept_id" options='{"no_open": True, "no_create": True}'/>
                            <field name="is_created" invisible="1"/>
                            <newline/>
                        </group>
                    </group>
                    <group>
                        <group>
                            <field name="from_cheque_no"/>
                            <field name="type" widget="radio"/>
                        </group>
                        <group>
                            <field name="to_cheque_no"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

